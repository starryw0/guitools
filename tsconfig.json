{
	"compilerOptions": {
		"outDir": "build",
		"target": "es5",
		"module": "esnext",
		"strict": true,
		"moduleResolution": "node",
		"lib": [
			"es5",
			"dom"
		],
		"declaration": true,
		"strictPropertyInitialization": false,
	},
	"include": [
		"src/**/*"
	],
	"exclude": [
		"node_modules",
		"src/test/**/*"
	]
}
